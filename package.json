  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/yup": "^0.28.0",
    "concurrently": "^5.1.0",
    "electron-builder": "^22.4.1"
  },
  "build": {
    "appId": "app.ca.mapleleafcollegiate",
    "copyright": "Copyright Â©2020 Maple Leaf Collegiate",
    "productName": "Maple Leaf Collegiate",
    "files": [
      "build/",
      "public/electron.ts",
      "!node_modules/"
    ],
    "mac": {
      "icon": "public/logo512.png",
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "public/logo512.png",
      "target": [
        "nsis",
        "msi"
      ]
    }
